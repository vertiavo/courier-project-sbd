<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="735.0" prefWidth="898.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.common.Controller">
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="dataLabel" text="Data">
               <padding>
                  <Insets left="5.0" />
               </padding>
            </Label>
            <TableView fx:id="table" prefHeight="682.0" prefWidth="698.0" />
         </children>
      </VBox>
   </center>
   <left>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="tableListLabel" text="List of tables">
               <padding>
                  <Insets left="5.0" />
               </padding>
            </Label>
            <ListView fx:id="listView" prefHeight="686.0" prefWidth="200.0" />
         </children>
      </VBox>
   </left>
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button fx:id="deleteButton" mnemonicParsing="false" text="Delete" />
            <Button fx:id="newButton" minWidth="51.0" mnemonicParsing="false" prefHeight="25.0" text="New" />
        </buttons>
         <padding>
            <Insets right="10.0" />
         </padding>
      </ButtonBar>
   </bottom>
</BorderPane>
